    "ldflex": "^2.12.0",
    "lodash": "^4.17.21",
    "marked": "^4.0.10",
    "rdflib": "^2.2.15",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-map-gl": "^6.1.18",
    "react-router-dom": "^5.2.0",
    "simpl-schema": "^1.10.2",
    "styled-components": "^5.2.1",
    "swr": "^0.5.4",
